<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <record id="sequence_hospital_patient" model="ir.sequence">
            <field name="name">Patient Sequence</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">HP</field>
            <field name="padding">5</field>
        </record>

        <record id="sequence_hospital_doctor" model="ir.sequence">
            <field name="name">Doctor Sequence</field>
            <field name="code">hospital.doctor</field>
            <field name="prefix">HD</field>
            <field name="padding">3</field>
        </record>

        <record id="sequence_hospital_appointment" model="ir.sequence">
            <field name="name">Appointment Sequence</field>
            <field name="code">hospital.appointment</field>
            <field name="prefix">HA</field>
            <field name="padding">3</field>
        </record>
    </data>
</odoo>